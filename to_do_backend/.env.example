# Backend environment

# Port for the Express API
PORT=4000

# Frontend origin allowed by CORS
FRONTEND_URL=http://localhost:3000

# Request logging level for morgan (e.g., 'dev', 'combined', 'tiny', or 'info' per docs)
LOG_LEVEL=info

# Healthcheck path
HEALTHCHECK_PATH=/health

# If running behind a proxy (like nginx), set to true
TRUST_PROXY=false
