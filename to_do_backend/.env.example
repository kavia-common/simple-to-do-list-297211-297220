# Backend environment variables
# Copy this file to ".env" and adjust as needed for local development.

# Port for the API server
PORT=4000

# CORS allowed origin (the frontend dev server)
FRONTEND_URL=http://localhost:3000

# Logger level/preset for morgan ("dev" for verbose, or "info" as requested)
LOG_LEVEL=info

# Healthcheck path
HEALTHCHECK_PATH=/health

# If behind a reverse proxy, set to true
TRUST_PROXY=false
